    <CheckBoxPreference
        android:key="pref_kalman"
        android:title="@string/pref_kalman_title"
        android:summary="@string/pref_kalman_summary"
        android:defaultValue="false" />
    <CheckBoxPreference
        android:key="pref_use_cell"
        android:title="@string/pref_use_cell_title"
        android:summary="@string/pref_use_cell_summary"
        android:defaultValue="true" />
    <CheckBoxPreference
