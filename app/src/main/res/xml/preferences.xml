        android:title="@string/pref_use_wlan_title"
        android:summary="@string/pref_use_wlan_summary"
        android:defaultValue="true" />
    <Preference
        android:key="pref_cull"
        android:title="@string/pref_cull_title"
        android:summary="@string/pref_cull_summary" />
    <Preference
        android:key="pref_import"
        android:title="@string/pref_import_title"
        android:summary="@string/pref_import_summary" />
    <Preference
        android:key="pref_export"
        android:title="@string/pref_export_title"
        android:summary="@string/pref_export_summary" />
